<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Circular outline -->
    <item>
        <shape android:shape="oval">
            <solid android:color="@android:color/transparent" />
            <size
                android:width="70dp"
                android:height="70dp" />
            <stroke
                android:width="2dp"
                android:color="@color/black" /> <!-- Change the color to the desired outline color -->
        </shape>
    </item>

    <!-- PNG image -->
    <item android:drawable="@drawable/diary" />

</layer-list>